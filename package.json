{
  "name": "dlib",
  "version": "0.0.0",
  "description": "Library for interacting with nan-tech's displacement database",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run build:browserify && npm run build:minify",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:browserify": "browserify lib/library/lib.js -o lib/dist.js",
    "build:minify": "./node_modules/uglify-js/bin/uglifyjs lib/dist.js -o lib/dist.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nan-tech/DLib.git"
  },
  "keywords": [
    "displacement-database",
    "nan-tech"
  ],
  "author": "Noah Rose Ledesma",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/nan-tech/DLib/issues"
  },
  "homepage": "https://github.com/nan-tech/DLib#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "browserify": "^16.2.3",
    "typescript": "^3.4.1",
    "uglify-js": "^3.5.3"
  },
  "dependencies": {
    "@types/geodesy": "^1.2.1",
    "firebase": "^5.9.3",
    "geodesy": "^1.1.3",
    "geofirestore": "^3.2.3"
  }
}
